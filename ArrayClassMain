public class ArrayClassMain
{  
   public static void main(String[] args)
   {
      ArrayClass array1 = new ArrayClass(20);    //Creates new array
      array1.fillArray();
      System.out.print("Unordered Array:\n" + array1.curElems());
      array1.selectionSort();
      System.out.print("\nOrdered Array:\n" + array1.curElems());
      System.out.println("\nLargest Number:" + array1.getLargest());
      System.out.println("\nArray With Largest Number Removed:\n" + array1.curElems());
      array1.deleteDups();
      System.out.println("\nArray With Duplicates Deleted:\n" + array1.curElems()); 
      
   }
   
   
}
